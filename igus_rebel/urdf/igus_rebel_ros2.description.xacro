<?xml version="1.0"?>
<robot xmlns:xacro="http://wiki.ros.org/xacro" name="igus_rebel_ros2_description">

    <xacro:macro name="igus_rebel_ros2_description">

        <xacro:property name="damping" value="0.1"/>
        <xacro:property name="friction" value="10.0"/>

        <link name="base_link">
            <visual>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint0.dae"/>
                </geometry>
            </visual>
            <collision>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint0.dae"/>
                </geometry>
            </collision>
            <inertial>
                <mass value="1.0" />
                <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0" />
            </inertial>
        </link>
        

        
        <joint name="joint1" type="revolute">
            <parent link="base_link"/>
            <child link="link_1"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0 0.100" />
            <axis xyz="0 0 1"/>
            <limit effort="330.0" lower="${-pi*179/180}" upper="${pi*179/180}" velocity="${pi*45/180}" />
            <dynamics damping="${damping}" friction="${friction}"/>
        </joint>
        
        <link name="link_1">
            <visual>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint1.dae"/>
                </geometry>
                <origin rpy="0 0 0" xyz="0 0 0" />
            </visual>
            <collision>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint1.dae"/>
                </geometry>
                <origin rpy="0 0 0" xyz="0 0 0" />
            </collision>
            <inertial>
                <mass value="0.1" />
                <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03" />
            </inertial>
        </link>
        
        
        <joint name="joint2" type="revolute">
            <parent link="link_1"/>
            <child link="link_2"/>
            <origin rpy="0.0 ${pi/6} 0.0" xyz="0.0 0 0.149" />
            <axis xyz="0 1 0"/>
            <limit effort="330.0" lower="${-pi*11/18}" upper="${pi*11/18}" velocity="${pi*45/180}" />
            <dynamics damping="${damping}" friction="${friction}"/>
        </joint>
        
        <link name="link_2">
            <visual>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint2.dae"/>
                </geometry>
                <origin rpy="0 0 0" xyz="0 0 0" />
            </visual>
            <collision>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint2.dae"/>
                </geometry>
                <origin rpy="0 0 0" xyz="0 0 0" />
            </collision>
            <inertial>
                <mass value="0.1" />
                <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03" />
            </inertial>
        </link>

        <joint name="joint3" type="revolute">
            <parent link="link_2"/>
            <child link="link_5"/>
            <origin rpy="0.0 ${pi/6} 0.0" xyz="0.0 0.0 0.2384" />
            <axis xyz="0 1 0"/>
            <limit effort="330.0" lower="${-pi*11/18}" upper="${pi*11/18}" velocity="${pi*45/180}" />
            <dynamics damping="${damping}" friction="${friction}"/>
        </joint>
        
        <link name="link_5">
            <visual>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint3.dae"/>
                </geometry>
                <origin rpy="0 0 0" xyz="0 0 0" />
            </visual>
            <collision>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint3.dae"/>
                </geometry>
                <origin rpy="0 0 0" xyz="0 0 0" />
            </collision>
            <inertial>
                <mass value="0.1" />
                <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03" />
            </inertial>
        </link>
        
        
        <joint name="joint4" type="revolute">
            <parent link="link_5"/>
            <child link="link_6"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0 ${0.149 - 0.03}" />
            <axis xyz="0 0 1"/>
            <limit effort="330.0" lower="${-pi*179/180}" upper="${pi*179/180}" velocity="${pi*45/180}" />
            <dynamics damping="${damping}" friction="${friction}"/>
        </joint>
        
        <link name="link_6">
            <visual>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint4.dae"/>
                </geometry>
                <origin rpy="0 0 0" xyz="0 0 0" />
            </visual>
            <collision>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint4.dae"/>
                </geometry>
                <origin rpy="0 0 0" xyz="0 0 0" />
            </collision>
            <inertial>
                <mass value="0.1" />
                <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03" />
            </inertial>
        </link>
        
        <joint name="joint5" type="revolute">
            <parent link="link_6"/>
            <child link="link_7"/>
            <origin rpy="0.0 ${-pi/24} 0.0" xyz="0.0 0 ${0.14 + 0.03}" />
            <axis xyz="0 1 0"/>
            <limit effort="330.0" lower="${-pi*19/36 + pi/24}" upper="${pi*19/36 + pi/24}" velocity="${pi*45/180}" />
            <dynamics damping="${damping}" friction="${friction}"/>
        </joint>
        
        <link name="link_7">
            <visual>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint5.dae"/>
                </geometry>
                <origin rpy="0 0 0" xyz="0 0 0" />
            </visual>
            <collision>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint5.dae"/>
                </geometry>
                <origin rpy="0 0 0" xyz="0 0 0" />
            </collision>
            <inertial>
                <mass value="0.1" />
                <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03" />
            </inertial>
        </link>
        
        
        <joint name="joint6" type="revolute">
            <parent link="link_7"/>
            <child link="link_8"/>
            <origin rpy="0.0 0.0 0.0" xyz="0.0 0 0.1208" />
            <axis xyz="0 0 1"/>
            <limit effort="330.0" lower="${-pi*179/180}" upper="${pi*179/180}" velocity="${pi*45/180}" />
            <dynamics damping="${damping}" friction="${friction}"/>
        </joint>
        
        <link name="link_8">
            <visual>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint6.dae"/>
                </geometry>
                <origin rpy="0 0 0" xyz="0.0 0 0" />
            </visual>
            <collision>
                <geometry>
                  <mesh filename="file://$(find igus_rebel)/meshes/robolink_rebel_ros2/Joint6.dae"/>
                </geometry>
                <origin rpy="0 0 0" xyz="0.0 0 0" />
            </collision>
            <inertial>
                <mass value="0.1" />
                <inertia ixx="0.03" ixy="0.0" ixz="0.0" iyy="0.03" iyz="0.0" izz="0.03" />
            </inertial>
        </link>

        <joint name="joint7" type="fixed">
            <parent link="link_8" />
            <child link="flange" />
            <origin rpy="0.0 ${-pi/2} 0.0" xyz="0.0 0 0.0012" />
            <axis xyz="0 0 0" />
        </joint>
        
        <link name="flange">
        </link>
    </xacro:macro>
</robot>
